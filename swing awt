import javax.swing.*;
class swing {
    swing(){
        JFrame f1=new JFrame("Registration");
        JButton b1=new JButton("Login");
        JLabel l1=new JLabel("Email");
        JTextArea t1=new JTextArea("Enter the Discription");
        f1.add(t1);
        f1.add(t1);
        f1.add(l1);
        f1.add(b1);
        f1.setVisible(true);
        f1.setSize(50,50);
    }
    public static void  main(String args[]){
        new swing();
    }
}
